{
  "name": "failure",
  "version": "0.0.3",
  "description": "Customizable Error replacement",
  "main": "main.js",
  "files": [
    "LICENSE",
    "main.js",
    "lib/*",
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:drslump/failure.git"
  },
  "dependencies": {
    "error-stack-parser": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^7.0.3 < 8",
    "mocha": "^2.1.0",
    "uglify-js": "^2.4.16"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --recursive test",
